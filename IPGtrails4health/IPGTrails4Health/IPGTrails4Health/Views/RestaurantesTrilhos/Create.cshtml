                <select asp-for="TrilhoId" class ="form-control" asp-items="ViewBag.TrilhoId"></select>
            </div>
            <div class="form-group">
                <label asp-for="RestauranteId" class="control-label"></label>
                <select asp-for="RestauranteId" class ="form-control" asp-items="ViewBag.RestauranteId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-outline-info">Voltar à Lista</a>
